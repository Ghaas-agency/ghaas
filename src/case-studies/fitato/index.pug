    //
      <script>
      ;(function() {
      var throttle = function(type, name, obj) {
      var obj = obj || window;
      var running = false;
      var func = function() {
      if (running) { return; }
      running = true;
      requestAnimationFrame(function() {
      obj.dispatchEvent(new CustomEvent(name));
      running = false;
      });
      };
      obj.addEventListener(type, func);
      };
      throttle ("scroll", "optimizedScroll");
      })();
      var phoneImg = document.getElementById('fitato-app-phone-img');
      var phoneContent = document.getElementById('fitato-app-phone-content');
      phoneContent.style.opacity = '0';
      phoneImg.style.transform = "translateX(102px)";
      var imgOffset = 0;
      var contentOffset = 0;
      window.addEventListener("optimizedScroll", function() {
      imgOffset = window.pageYOffset * 0.015;
      contentOffset = 102 - (imgOffset*15);
      if(imgOffset < 7) {
      /* phoneImg.style.transform = "rotate(-"+imgOffset+"deg)"; */
      phoneImg.style.transform = "translateX("+contentOffset+"px)";
      phoneContent.style.opacity = '0';
      } else {
      phoneContent.style.opacity = '1';
      };
      });00
      </script>
