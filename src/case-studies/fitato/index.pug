      //-       .cs-fitato-results__item
      //-         img(src="../../assets/img/case-studies/fitato/fitato-push-notification.png" width="500" alt="")
      section.cs-fitato-results
        .container
          .cs-fitato-results__wrapper
            img.cs-fitato-results--full(src="../../assets/img/case-studies/fitato/fitato-mockup.jpg" alt="fitato results screens")
            img.cs-fitato-results--mobile(src="../../assets/img/case-studies/fitato/fitato-mockup-mobile.jpg" alt="fitato results screens")
      section.cs-fitato-tools
        .container
          .line
          h2 Platforms/Tools Used
          .cs-fitato-tools__tools
            .cs-fitato-tools__tool
              p Content Marketing
            .cs-fitato-tools__tool
              p Facebook Ads
            .cs-fitato-tools__tool
              p Google Ads
            .cs-fitato-tools__tool
              p Website Optimisation
            .cs-fitato-tools__tool
              p Landing Pages/CRO
            .cs-fitato-tools__tool
              p Search Engine Optimization
            .cs-fitato-tools__tool
              p Mobile Marketing
            .cs-fitato-tools__tool
              p Email Marketing
            .cs-fitato-tools__tool
              p App Store Optimization
            .cs-fitato-tools__tool
              p Mobile Analytics
            .cs-fitato-tools__tool
              p Growth Model
            .cs-fitato-tools__tool
              p Community Building

